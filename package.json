{
  "name": "@christophern/populartimesjs",
  "version": "1.0.3",
  "description": "Retrieve popular times and current popularity of google places by its id.",
  "main": "dist/populartimes.js",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon",
    "prebuild": "npm run test",
    "build": "tsc",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "preversion": "npm run build",
    "postversion": "git push"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ChristopherNeuwirth/populartimesjs"
  },
  "bugs": {
    "url": "https://github.com/ChristopherNeuwirth/populartimesjs/issues"
  },
  "homepage": "https://github.com/ChristopherNeuwirth/populartimesjs#readme",
  "keywords": [
    "populartimes",
    "google",
    "places",
    "javascript",
    "js",
    "service",
    "maps",
    "time"
  ],
  "typings": "dist/populartimes",
  "author": "Christopher Neuwirth <npm@dev.cneuwirth.de>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "jsdom": "^16.2.2",
    "puppeteer": "^3.3.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.0",
    "@types/jsdom": "^16.2.3",
    "@types/node": "^12.12.47",
    "@types/puppeteer": "^3.0.0",
    "dotenv": "^8.2.0",
    "jest": "^26.0.1",
    "nodemon": "^1.19.4",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "typescript": "^3.9.5"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "files": [
    "dist/",
    "!dist/**/*.test.*"
  ]
}
