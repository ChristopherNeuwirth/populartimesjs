{
  "name": "populartimesjs",
  "version": "1.0.0",
  "description": "Retrieve popular times and current popularity of google places by its id.",
  "main": "dist/index.js",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ChristopherNeuwirth/populartimesjs"
  },
  "bugs": {
    "url": "https://github.com/ChristopherNeuwirth/populartimesjs/issues"
  },
  "engines": {
    "node": ">=12.14.0"
  },
  "keywords": [
    "populartimes",
    "google",
    "places",
    "javascript",
    "js",
    "service"
  ],
  "author": "Christopher Neuwirth <christopher.neuwirth@mhp.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "jsdom": "^16.2.2",
    "puppeteer": "^3.3.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/jsdom": "^16.2.3",
    "@types/node": "^12.12.47",
    "@types/puppeteer": "^3.0.0",
    "nodemon": "^1.19.4",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "typescript": "^3.9.5"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
