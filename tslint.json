{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [
      true
    ],
    "no-console": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-unused-expression": false,
    "no-string-literal": false,
    "no-empty": false
  },
  "rulesDirectory": []
}